{
  "expo": {
    "name": "kniyotes",
    "slug": "kniyotes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kniyotes",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UISupportsDocumentBrowser": false,
        "UIViewSemanticContentAttribute": "Unspecified",
        "UIViewAllowRTL": true,
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.avirankatz.kniyotes"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.avirankatz.kniyotes"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "supportsRTL": true,
      "router": {},
      "eas": {
        "projectId": "94e0b716-cdcb-4561-a0a6-46fe93dfe189"
      }
    }
  }
}